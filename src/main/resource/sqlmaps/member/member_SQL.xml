<mapper namespace="com.team3.inlecture.member.MemberMapper">
    <insert id="insertMember" parameterType="memberVO">
    	INSERT INTO MEMBER 
    	(ID, PW, NAME, NICK, ROLE, SCHOOL, EMAIL) VALUES 
    	(#{id}, #{pw}, #{name}, #{nick}, #{role}, #{school}, #{email}) 
